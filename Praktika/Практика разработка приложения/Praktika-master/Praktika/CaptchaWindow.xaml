<Window x:Class="Praktika.CaptchaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktika"
        mc:Ignorable="d"
         ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="Каптча" Height="250" Width="450"
        WindowStyle="None">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="340">
            <TextBlock Name="textBlock"  Margin="0 20" TextAlignment="Center" FontSize="56"/>
            <TextBox x:Name="textBoxCaptcha" KeyDown="textBoxCaptcha_KeyDown"/>
        </StackPanel>
    </Grid>
</Window>
